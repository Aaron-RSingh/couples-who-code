<%= form_for (@user) do |f| %>
<ul>
  <li><%= f.label  :email %>
  <%= f.text_field :email %></li>

  <li><%= f.label  :create_a_secure_password%>
  <%= f.password_field :password %></li>
  
  <li><%= f.label  :first_name %>
  <%= f.text_field :first_name %></li>

  <li><%= f.label  :last_name %>
  <%= f.text_field :last_name %></li>

  <li><%= f.label :age %>
  <%= f.number_field :age %></li>

  <li><%= f.label :tagline %>
  <%= f.text_area :tagline %></li>
  
  <li><%= f.label :bio %>
  <%= f.text_area :bio %></li>

  <li><%= f.label :picture %>
  <%= f.file_field :picture %></li>
  
</ul>
  <%= f.submit %>
  <% end %>

  





